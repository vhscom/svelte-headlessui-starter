{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$components": ["src/lib/shared/components/index.ts"],
			"$components/*": ["src/lib/shared/components/*"],
			"$core": ["src/lib/core"],
			"$core/*": ["src/lib/core/*"],
			"$layouts": ["src/lib/shared/layouts/index.ts"],
			"$layouts/*": ["src/lib/layouts/*"],
			"$data": ["src/lib/data/index.ts"],
			"$data/*": ["src/lib/data/*"],
			"$models": ["src/lib/models/index.ts"],
			"$models/*": ["src/lib/models/*"],
			"$utils": ["src/lib/utils/index.ts"],
			"$utils/*": ["src/lib/utils/*"],
			"$environment": ["src/environment/index.ts"]
		}
	}
}
