		"postcss": "^8.4.6",
		"postcss-load-config": "^3.1.3",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
		"prettier-plugin-tailwindcss": "^0.1.7",
		"sk-auth": "^0.4.1",
		"svelte": "^3.46.4",
		"svelte-check": "^2.4.5",
		"svelte-meta-tags": "^2.3.0",
		"svelte-preprocess": "^4.10.4",
		"tailwindcss": "^3.0.23",
		"ts-md5": "^1.2.11",
		"tslib": "^2.3.1",
		"typescript": "~4.5.5",
		"unplugin-icons": "^0.13.2",
		"vercel": "^24.0.0"
	},
	"type": "module"
